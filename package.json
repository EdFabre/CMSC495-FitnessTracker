{
  "name": "fittrac",
  "productName": "Fittrac",
  "version": "1.0.1",
  "description": "CMSC495 Fitness Tracking App",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "keywords": [],
  "author": "enfabre",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "out": "Releases",
        "platform": "all",
        "dir": "src"
      },
      "electronWinstallerConfig": {
        "name": "Fittrac",
        "setupExe": "FittracSetup.exe",
        "setupIcon": "src/assets/icons/win/fittrac.ico"
      },
      "electronInstallerDMG": {
        "name": "Fittrac",
        "icon": "src/assets/icons/mac/fittrac.icns",
        "format": "ULFO"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "EdFabre",
        "name": "CMSC495-FitnessTracker"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Fittrac"
      }
    }
  },
  "dependencies": {
    "bmi-calc": "^2.0.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.1",
    "cloudinary": "^1.9.1",
    "config": "^1.28.1",
    "electron-compile": "^6.4.2",
    "electron-context-menu": "^0.9.1",
    "electron-devtools-installer": "^2.1.0",
    "electron-dl": "^1.10.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^1.3.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "nodemon": "^1.12.5",
    "parse": "^1.10.2",
    "react": "^15.4.1",
    "react-alert": "^2.4.0",
    "react-big-calendar": "^0.17.0",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-button-loader": "^1.0.10",
    "react-calendar": "^2.3.1",
    "react-datetime": "^2.11.1",
    "react-day-picker": "^7.0.5",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^15.4.1",
    "react-dropzone": "^4.2.1",
    "react-edit-inline": "^1.0.8",
    "react-edit-inplace": "^1.1.1",
    "react-fader": "^1.1.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-images-uploader": "^1.1.0",
    "react-table": "^6.7.4",
    "react-tabs-redux": "^2.0.1",
    "react-widgets": "^4.1.1",
    "rekuire": "^0.1.9",
    "riek": "^1.1.0",
    "styled-components": "^2.2.3",
    "superagent": "^3.8.1",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2",
    "winston-electron": "^0.2.0",
    "winston-loggly": "^1.3.1",
    "winston-loggly-bulk": "^2.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.1.5",
    "electron-packager": "^10.1.0",
    "electron-prebuilt-compile": "1.7.9",
    "electron-winstaller": "^2.6.3",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7"
  }
}
